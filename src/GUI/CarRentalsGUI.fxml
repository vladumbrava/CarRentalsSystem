<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.CarRentalsGUIController">
   <ListView fx:id="carListView" layoutX="14.0" layoutY="14.0" prefHeight="151.0" prefWidth="600.0" />
   <ListView fx:id="rentalListView" layoutX="14.0" layoutY="210.0" prefHeight="175.0" prefWidth="293.0" />
   <Button fx:id="addCarButton" layoutX="584.0" layoutY="177.0" mnemonicParsing="false" onAction="#addCarButtonHandler" text="Add Car" />
   <TextField fx:id="carModelNameTextField" layoutX="14.0" layoutY="177.0" prefHeight="26.0" prefWidth="114.0" promptText="Model Name" />
   <TextField fx:id="carHorsePowerTextField" layoutX="143.0" layoutY="177.0" prefHeight="26.0" prefWidth="86.0" promptText="Horse Power" />
   <ComboBox fx:id="carNumberSeatsComboBox" layoutX="242.0" layoutY="177.0" promptText="Number of seats" />
   <ComboBox fx:id="carFuelTypeComboBox" layoutX="386.0" layoutY="177.0" promptText="Fuel Type" />
   <ComboBox fx:id="carColourComboBox" layoutX="492.0" layoutY="177.0" prefHeight="26.0" prefWidth="79.0" promptText="Colour" />
   <Button fx:id="deleteCarButton" layoutX="800.0" layoutY="20.0" mnemonicParsing="false" onAction="#deleteCarButtonHandler" text="Delete Car" />
   <TextField fx:id="carModelNameTextFieldToDelete" layoutX="628.0" layoutY="20.0" promptText="Model Name" />
</AnchorPane>
